#!/bin/bash
#------------------------------------------------------------------------------
# Removes project .settings generated by eclipse
#------------------------------------------------------------------------------
set -e

DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"

pushd "$DIR" > /dev/null

rm -rf ../ceri-common/.settings
rm -rf ../ceri-log/.settings
rm -rf ../ceri-ent/.settings
rm -rf ../ceri-process/.settings
rm -rf ../ceri-serial/.settings
rm -rf ../ceri-x10/.settings
rm -rf ../ceri-zwave/.settings
rm -rf ../ceri-misc/.settings
rm -rf ../ceri-ci/.settings

popd > /dev/null
