CC=@CC@
CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@

all: lib

install: lib
	@INSTALL@ -c src/libpcirl.a /usr/local/lib
	@INSTALL@ -c src/pcirlinc.h /usr/local/include

lib:
	cd src; $(MAKE) lib

clean:
	cd src; $(MAKE) clean

distclean:
	cd src; $(MAKE) distclean;
	rm -f config.cache config.h config.status config.log Makefile

